
<div style="float:top;float:right">
<%= render 'form'%>
</div>

<h1>All Students In MUN</h1>

<table>
  <thead>
    <tr>
      <th>Picture-</th>
      <th>First Name-</th>
      <th>-Middle Name-</th>
      <th>-Last Name-</th>
      <th>-Graduating Year</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><img src="<%= student.pic_url%> " style="height:10em;width:10em"></td>
        <td><%= student.first_name %></td>
        <td><%= student.middle_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.grad_class %></td>
        <td><%= link_to 'Bigger-', student %></td>
        <td><%= link_to '-Edit', edit_student_path(student.id) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>




<%= link_to 'New Student', new_student_path %>
